{
  "name": "patient-app-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "(npm run generate:api || true) && concurrently \"next dev --turbopack\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "i18n": "i18next-resources-for-ts interface -i ./src/locales/en -o ./@Types/resources.d.ts",
    "generate:api": "openapi-generator-cli generate -g typescript-axios -i http://localhost:8080/v3/api-docs -o src/api --additional-properties=supportsES6=true,withInterfaces=true,withSeparateModelsAndApi=true,apiPackage=apis,modelPackage=models --skip-validate-spec"
  },
  "dependencies": {
    "axios": "^1.10.0",
    "i18next": "^25.1.2",
    "i18next-resources-for-ts": "^1.6.0",
    "i18next-resources-to-backend": "^1.2.1",
    "next": "^15.3.2",
    "next-i18n-router": "^5.5.1",
    "react": "^19.0.0",
    "react-chatbot-kit": "^2.2.2",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.5.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@openapitools/openapi-generator-cli": "^2.18.4",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.2.2",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}